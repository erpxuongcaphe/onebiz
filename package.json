{
  "name": "onebiz-erp---mobile-first-edition",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:main": "vite --mode main",
    "dev:pos": "vite --mode pos",
    "build": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build",
    "build:main": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build --mode main",
    "build:pos": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build --mode pos",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
