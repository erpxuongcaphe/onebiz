{
  "name": "onebiz-erp---mobile-first-edition",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:main": "vite --mode main",
    "dev:pos": "vite --mode pos",
    "build": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build",
    "build:main": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build --mode main",
    "build:pos": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build --mode pos",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.1",
    "@types/js-cookie": "^3.0.6",
    "html2pdf.js": "^0.14.0",
    "idb": "^8.0.3",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.563.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
